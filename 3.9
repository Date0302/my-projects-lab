class Student {
5        private String name;  // 定义私有成员变量 name
6        private int age;      // 定义私有成员变量 age
7    
8        // 带参构造方法（初始化 name 和 age）
9        public Student(String name, int age) {
10           this.name = name;  // 使用 this 区分成员变量和局部变量
11           this.age = age;     // 修正赋值语句
12       }
13   
14       // 成员方法：sayHello()
15       public void sayHello() {
16           System.out.println("我是：" + name + "，年龄：" + age);
17       }
18   }
19   
20   public class Example9 {
21       public static void main(String[] args) {
22           Student stu = new Student("张海", 21);  // 实例化 Student 对象
23           stu.sayHello();  // 调用 sayHello() 方法
24       }
25   }
